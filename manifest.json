{
  "manifest_version": 2,

  "name": "Cydia Store Gift",
  "description": "Gifts packages on the Cydia Store",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "webNavigation",
    "https://cydia.saurik.com/*"
  ]
}